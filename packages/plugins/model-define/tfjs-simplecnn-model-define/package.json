{
  "name": "@pipcook/plugins-tfjs-simplecnn-model-define",
  "version": "1.0.0",
  "description": "This plugin defines a image classification model. This model is built on top of several convolutional blocks. This model is fit for quite simple dataset. The model is built using Tensorflow.js.",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pipcook/pipcook-core": "^1.0.0",
    "@tensorflow/tfjs-node-gpu": "1.7.0",
    "jimp": "^0.10.0"
  },
  "gitHead": "53e131a069b3112a74ae5d99aa1ab08bec768c7e",
  "devDependencies": {
    "@types/jasmine": "^3.5.7",
    "nyc": "14.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "pipcook": {
    "category": "modelDefine",
    "datatype": "image",
    "params": [{
      "name": "loss",
      "type": "string[]",
      "options": [
        "meanSquaredError",
        "meanAbsoluteError",
        "meanAbsolutePercentageError",
        "meanSquaredLogarithmicError",
        "squaredHinge",
        "hinge",
        "categoricalHinge",
        "logcosh",
        "categoricalCrossentropy",
        "sparseCategoricalCrossentropy",
        "binaryCrossentropy",
        "kullbackLeiblerDivergence",
        "poisson",
        "cosineProximity"
      ],
      "defaultValue": [ "categoricalCrossentropy" ],
      "description": "the loss function to be defined"
    }, {
      "name": "metrics",
      "type": "string[]",
      "options": [
        "accuracy",
        "recall"
      ],
      "defaultValue": [ "accuracy" ],
      "description": "metrics used to evaluate model."
    }]
  }
}
